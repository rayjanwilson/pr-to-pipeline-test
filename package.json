{
  "name": "pr-to-pipeline-test",
  "version": "0.1.2",
  "engines": {
    "node": ">=12.9.0"
  },
  "bin": {
    "pr-to-pipeline": "bin/pr-to-pipeline.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "commit": "git-cz",
    "deploy": "npm run build && npx cdk deploy --require-approval never",
    "lint": "eslint '*/**/*.ts' --ignore-pattern '*/**/*.d.ts' --ignore-pattern 'cdk.out/*'",
    "release": "HUSKY=0 standard-version",
    "test": "jest",
    "upgrade": "npx npm-check-updates -u",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.109.0",
    "@mhlabs/cfn-diagram": "^1.1.26",
    "@mhlabs/cfn-diagram-ci": "0.0.2",
    "@types/aws-lambda": "^8.10.77",
    "@types/jest": "^26.0.23",
    "@types/node": "15.12.3",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "aws-cdk": "1.109.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.12.9",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "standard-version": "^9.3.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "~4.3.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.109.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.109.0",
    "@aws-cdk/aws-codebuild": "^1.109.0",
    "@aws-cdk/aws-codepipeline": "^1.109.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.109.0",
    "@aws-cdk/aws-ec2": "^1.109.0",
    "@aws-cdk/aws-ecr": "^1.109.0",
    "@aws-cdk/aws-iam": "^1.109.0",
    "@aws-cdk/aws-imagebuilder": "^1.109.0",
    "@aws-cdk/aws-lambda": "^1.109.0",
    "@aws-cdk/aws-logs": "^1.109.0",
    "@aws-cdk/aws-s3": "^1.109.0",
    "@aws-cdk/core": "1.109.0",
    "@aws-cdk/pipelines": "^1.109.0",
    "source-map-support": "^0.5.19"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
